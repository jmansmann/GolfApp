@page
@model GolfApp.Pages.AboutModel

@{
    ViewData["Title"] = "Course Statistics";
}

<h2>Course Founding Statistics (test)</h2>

<table>
    <tr>
        <th>
            Founded Date
        </th>
        <th>
            Courses
        </th>
    </tr>

    @foreach (var item in Model.Courses)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateFounded)
            </td>
            <td>
                @item.CourseCount
            </td>
        </tr>
    }
</table>